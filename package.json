{
  "name": "life-helper-backend",
  "version": "0.9.13",
  "description": "「我的个人助手」小程序服务端部分代码",
  "main": "index.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inlym/life-helper-backend.git"
  },
  "author": "inlym",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/inlym/life-helper-backend/issues"
  },
  "homepage": "https://github.com/inlym/life-helper-backend#readme",
  "scripts": {
    "build": "nest build",
    "clean": "rimraf dist",
    "update-config": "npm i life-helper-config",
    "git:push": "git push && git push --tag",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:pm2:dev": "pm2 start pm2.json",
    "launch:pm2:prod": "pm2 reload pm2.json --env=production",
    "launch:docker:prod": "cross-env NODE_ENV=production node dist/src/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "bombdb": "typeorm schema:drop && typeorm schema:sync"
  },
  "dependencies": {
    "@nestjs/cli": "^7.6.0",
    "@nestjs/common": "^7.6.15",
    "@nestjs/core": "^7.6.15",
    "@nestjs/platform-express": "^7.6.15",
    "@nestjs/schedule": "^0.4.3",
    "@nestjs/schematics": "^7.3.1",
    "@nestjs/swagger": "^4.8.0",
    "@nestjs/typeorm": "^7.1.5",
    "ali-oss": "^6.15.2",
    "axios": "^0.21.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "cross-env": "^7.0.3",
    "dayjs": "^1.8.36",
    "jshttp": "^0.4.2",
    "life-helper-config": "^2.9.2",
    "mysql2": "^2.2.5",
    "nestjs-redis": "^1.3.3",
    "pm2": "^4.5.6",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.6",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.32",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.11",
    "@nestjs/testing": "^7.6.17",
    "@types/ali-oss": "^6.0.9",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.36",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "prettier": "^2.3.0",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.0.18",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
