{
  "name": "life-helper-backend",
  "version": "1.0.0",
  "description": "「我的个人助手」小程序服务端部分代码。",
  "main": "index.js",
  "directories": {
    "doc": "doc"
  },
  "egg": {
    "declarations": true
  },
  "scripts": {
    "start": "egg-scripts start --daemon --env=prod --title=lh-prod",
    "stop": "egg-scripts stop --title=lh-prod",
    "reload": "npm run stop && npm run start",
    "dev": "egg-bin dev",
    "prodev": "EGG_SERVER_ENV=prod egg-bin dev",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "test-temp": "egg-bin test test/temp",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod",
    "apidoc": "apidoc -i app/controller/ -o apidoc/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inlym/life-helper-backend.git"
  },
  "author": "inlym",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/inlym/life-helper-backend/issues"
  },
  "homepage": "https://github.com/inlym/life-helper-backend#readme",
  "apidoc": {
    "name": "life-helper-doc",
    "version": "1.0.0",
    "description": "我的个人助手小程序项目后端 API 接口文档",
    "url": "https://doc.lh.inlym.com",
    "apidoc": {
      "title": "我的个人助手小程序项目后端 API 接口文档",
      "url": "https://doc.lh.inlym.com"
    }
  },
  "devDependencies": {
    "apidoc": "^0.26.0",
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "cross-env": "^7.0.2",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.11.0",
    "egg-mock": "^3.21.0",
    "eslint": "^5.13.0",
    "eslint-config-egg": "^7.1.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.1.2",
    "sequelize-cli": "^6.2.0"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "dayjs": "^1.9.5",
    "egg": "^2.15.1",
    "egg-alinode": "^2.0.1",
    "egg-aliyun-tablestore": "^1.1.0",
    "egg-apigw-tracer": "^1.1.7",
    "egg-kit": "0.0.4",
    "egg-load": "^1.1.0",
    "egg-mysql": "^3.0.0",
    "egg-oss": "^2.0.0",
    "egg-redis": "^2.4.0",
    "egg-scripts": "^2.11.0",
    "egg-sequelize": "^6.0.0",
    "egg-validate": "^2.0.2",
    "egg-view-nunjucks": "^2.2.0",
    "mysql2": "^2.2.5",
    "only": "0.0.2",
    "only2": "^1.0.0",
    "spawn-object": "^1.1.0"
  }
}
